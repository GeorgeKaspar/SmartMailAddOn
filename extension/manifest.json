{
  "manifest_version": 2,
  "name": "SmartMail Events",
  "version": "0.1",
  "browser_action": {
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "browsingData",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [{
      "matches": [
        "https://e.mail.ru/messages/inbox/*"
      ],
      "js": ["user.js"]
    }
  ]
}
